<div class="flex flex-col justify-center">

  <span>Welcome to Stock APP</span>

  <%= button_to "Next", sample_page_path, class: "btn-primary" %>
  <%= button_to "Logout", signout_path, class: "btn-primary" %>

  <div id="container">
    
  </div>

<%= turbo_frame_tag "chart" do%>
  <%= candlestick_chart({
    '2017-10-24' => [20, 34, 10, 38],
    '2017-10-25' => [40, 35, 30, 50],
    '2017-10-26' => [31, 38, 33, 44],
    '2017-10-27' => [38, 15, 5, 42]
  },
  class: 'box',
  theme: 'roma',
  options: {
    xAxis: {
      axisTick: {
        alignWithLabel: true
      }
    }
  })
  %>
<% end %>

</div>